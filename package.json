{
  "name": "input-material-ui",
  "version": "2.1.0",
  "description": "A material-ui INPUT component which allows to see/hide the password text of the field and to clear the input value",
  "main": "lib/index.js",
  "sideEffects": false,
  "scripts": {
    "build": "rimraf ./lib && tsc",
    "deploy": "cross-env NODE_ENV=production npm run build && npm publish",
    "test": "echo \"Error: no test specified\" && exit 1",
    "storybook": "start-storybook -c storybook",
    "build-storybook": "build-storybook -c storybook -o docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iulian-radu-at/input-material-ui.git"
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "keywords": [
    "text",
    "input",
    "password",
    "material-ui",
    "input-material-ui",
    "react",
    "react-component",
    "ui",
    "material design"
  ],
  "author": "Iulian Radu <iulian.radu@gmx.at>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iulian-radu-at/input-material-ui/issues"
  },
  "homepage": "https://github.com/iulian-radu-at/input-material-ui#readme",
  "dependencies": {
    "@material-ui/core": "^4.2.0",
    "@material-ui/icons": "^4.2.1",
    "lodash": "^4.17.14",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.5.4",
    "@storybook/react": "^5.1.9",
    "@types/babel-core": "^6.25.6",
    "@types/enzyme": "^3.10.2",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/lodash": "^4.14.136",
    "@types/react": "^16.8.23",
    "@types/react-dom": "^16.8.4",
    "@types/rimraf": "^2.0.2",
    "@types/storybook__react": "^4.0.2",
    "babel-core": "^6.26.3",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3"
  },
  "peerDependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "./node_modules",
      "./history"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest"
    },
    "testRegex": "src/*(/(__tests?__/)([^_].*/)*?[^_][^/]*?\\.(test|spec)?\\.(js?))$",
    "setupFiles": [
      "./enzymeAdapter.setup.js"
    ]
  }
}
